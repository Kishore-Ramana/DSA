#include <stdio.h>

int main() {
    int n, i, key, pos = -1;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int a[n];
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter element to search: ");
    scanf("%d", &key);

    for (i = 0; i < n; i++) {
        if (a[i] == key) {
            pos = i;
            break;
        }
    }

    if (pos == -1)
        printf("Element not found.\n");
    else
        printf("Element found at index %d.\n", pos);

    return 0;
}
